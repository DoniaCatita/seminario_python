    "num = input(\"Ingrese su tiempo de reacción en ms: \")\n",
    "if(num.isnumeric()):\n",
    "    num = int(num)\n",
    "    if num < 200:\n",
    "        print(\"Categoria: rapido\")\n",
    "    elif num > 500:\n",
    "        print(\"Categoria: lento\")\n",
    "    else:\n",
    "        print(\"Categoria: normal\")\n",
    "else:\n",
    "    print(\"Valor no valido\")"
